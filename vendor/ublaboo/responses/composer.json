{
	"name": "ublaboo/responses",
	"type": "library",
	"description": "Additional responses for Nette Framework",
	"keywords": ["responses", "nette", "csv", "image", "json"],
	"homepage": "http://ublaboo.paveljanda.com/responses",
	"license": ["MIT"],
	"support": {
		"issues": "https://github.com/ublaboo/responses/issues"
	},
	"authors": [
		{
			"name": "Pavel Janda",
			"homepage": "http://paveljanda.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Ublaboo\\Responses\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ublaboo\\Responses\\Tests\\": "tests"
		}
	},
	"require": {
		"nette/http"  : "^2.3|^2.4",
		"nette/utils" : "^2.3|^2.4"
	},
	"suggest": {
		"psr/http-message": "Allows usage of PSR7 stream response."
	}
}
